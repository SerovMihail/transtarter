{
  "name": "transtarter-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest ./dist/app",
    "build:staging": "vue-cli-service build --dest ./dist/app --mode staging",
    "build:prod": "vue-cli-service build --dest ./dist/app --mode production",
    "lint": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit || exit 0",
    "test:unit:debug": "node --inspect-brk ./node_modules/@vue/cli-service/bin/vue-cli-service.js test:unit --runInBand",
    "test:e2e:all": "testcafe all ./tests/e2e/*.test.js --app \"npm run serve  -- --mode staging\" --app-init-delay 10000",
    "test:e2e": "testcafe chrome ./tests/e2e/*.test.js --app \"npm run serve  -- --mode staging\" --app-init-delay 10000",
    "wc": "vue-cli-service build --target wc --name ts-ui ./src/components/**/*.vue --dest ./dist/wc-dev",
    "wc:header-only": "vue-cli-service build --target wc --name ts-ui-header --mode development ./src/components/shared/header/header.vue --dest ./dist/wc-dev",
    "wc:prod": "vue-cli-service build --target wc --name ts-ui ./src/components/**/*.vue --mode production --dest ./dist/wc",
    "wc-unix:prod": "vue-cli-service build --target wc --name ts-ui './src/components/**/*.vue' --mode production --dest ./dist/wc",
    "wc:staging": "vue-cli-service build --target wc --name ts-ui ./src/components/**/*.vue --mode staging --dest ./dist/wc-staging",
    "wc-unix:staging": "vue-cli-service build --target wc --name ts-ui './src/components/**/*.vue' --mode staging --dest ./dist/wc-staging",
    "wc-unix-mini": "vue-cli-service build --target wc --name ts-ui-header './src/components/shared/header/header.vue'",
    "wc-unix-mini:dev": "vue-cli-service build --target wc --name ts-ui-header --mode development './src/components/shared/header/header.vue'"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cookie-storage-domain": "^1.0.4",
    "extract-domain": "^2.0.6",
    "faker": "^4.1.0",
    "oidc-client": "^1.10.1",
    "style-loader": "^1.1.3",
    "v-mask": "^1.3.4",
    "vue": "^2.6.11",
    "vue-carousel": "^0.17.0",
    "vue-class-component": "^6.0.0",
    "vue-mq": "^1.0.1",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.1.6",
    "vue-yandex-maps": "^0.8.18",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-typescript": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^3.2.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "@vue/web-component-wrapper": "^1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "element-ui": "^2.13.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^5.2.3",
    "faker": "^4.1.0",
    "html-loader": "^0.5.5",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.13.1",
    "prettier": "1.17.0",
    "sass-loader": "^7.3.1",
    "sinon": "^7.5.0",
    "testcafe": "^1.8.3",
    "testcafe-vue-selectors": "^3.0.0",
    "ts-jest": "^23.0.0",
    "typescript": "~3.1.1",
    "v-click-outside": "^2.1.5",
    "v-tooltip": "^2.0.3",
    "vue-template-compiler": "^2.6.11",
    "vuex-module-decorators": "^0.9.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
